{
    "name": "newtab for me",
    "description": "newtab",
    "version": "0.21",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "icons": {
        "16": "resource/icons/texteditor_16.png",
        "24": "resource/icons/texteditor_24.png",
        "48": "resource/icons/texteditor_48.png",
        "128": "resource/icons/texteditor_128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "topSites"
    ],

    "app": {
        "launch": {
            "local_path": "index.html#launch"
        }
    }
}

